{
  "name": "calypso-trade-status-dashboard",
  "version": "1.0.0",
  "description": "Real-time trade status dashboard for Calypso trading platform",
  "main": "src/backend/server.js",
  "scripts": {
    "start": "node src/backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
